#include "holberton.h"
/**
 * _islower - prints 1 if char is lowercase or o if not
 * @c: The character to say if is lower
 * Return: 0 if compilation is succesfully
 */
void print_square(int size)
{

	int i, j;
	for (i = 0; i < size; i++)
	{
		if (size > 0)
		{
			for (j = 0; j < size; j++)
			{
				_putchar(35);
			}
			_putchar('\n');
		}
	}
	if (size <= 0)
	{
		_putchar('\n');
	}
}
